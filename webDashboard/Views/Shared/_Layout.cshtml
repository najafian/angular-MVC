<!DOCTYPE html>
<html>
<head id="headerTag">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - webDashboard</title>
    <base href="~/" />
    <link href="dist/Content/js/gridstack/gridstack.min.css" rel="stylesheet" />
    <script src="dist/Content/js/jquery/jquery.min.js"></script>
    <script src="dist/Content/js/gridstack/lodash.min.js"></script>
    <script src="dist/Content/js/gridstack/jquery-ui.js"></script>
    <script src="dist/Content/js/gridstack/gridstack.min.js"></script>
    <script src="dist/Content/js/gridstack/gridstack.jQueryUI.min.js"></script>
    @*<link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />*@
    <link href="dist/Content/styles/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="dist/Content/styles/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link id="themePageStyle" href="~/dist/Content/styles/ej/web/default-theme/ej.web.all.css" rel="stylesheet" />
    <script src="dist/Content/js/syncfusion/ej.web.all.min.js"></script>
    <style>
        html, body {
            height: 100%;
        }

        .container {
            height: 100%;
            width: 100%;
            padding: 0px;
            margin: 0px;
        }

        .fill {
            width: 100%;
            height: 100%;
            min-height: 100%;
            padding: 0px;
        }
    </style>
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
